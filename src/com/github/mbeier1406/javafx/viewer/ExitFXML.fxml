<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="150.0" prefWidth="200.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.171" fx:controller="com.github.mbeier1406.javafx.viewer.ExitController">
   <children>
      <Label layoutX="33.0" layoutY="29.0" text="Wirklich beenden?">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <Button fx:id="exitButton" layoutX="14.0" layoutY="88.0" mnemonicParsing="false" onAction="#buttonTapped" text="Beenden" />
      <Button fx:id="abortButton" layoutX="99.0" layoutY="88.0" mnemonicParsing="false" onAction="#buttonTapped" text="Abbrechen" />
   </children>
</AnchorPane>
